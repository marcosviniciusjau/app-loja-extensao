{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/screens/CashClosing/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@dao/*": [
        "./src/dao/*"
      ],
      "@db/*": [
        "./src/db/*"
      ],
      "@dtos/*": [
        "./src/dtos/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@/*": [
        "./*"
      ],

    }
  }
}