{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@dao/*": [
        "./src/dao/*"
      ],
      "@db/*": [
        "./src/db/*"
      ],
      "@dtos/*": [
        "./src/dtos/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ]
    }
  }
}
